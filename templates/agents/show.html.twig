<!DOCTYPE html>

<title>Agents</title>

{% block body %}
    <h1>Agents</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agent.id }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ agent.lastName }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ agent.firstName }}</td>
            </tr>
            <tr>
                <th>CodeName</th>
                <td>{{ agent.codeName }}</td>
            </tr>
            <tr>
                <th>Skills</th>
                <td>{{ agent.skills ? agent.skills|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>DateOfBirth</th>
                <td>{{ agent.dateOfBirth ? agent.dateOfBirth|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Nationality</th>
                <td>{{ agent.nationality }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('agents_index') }}">back to list</a>

    <a href="{{ path('agents_edit', {'id': agent.id}) }}">edit</a>

    {{ include('agents/_delete_form.html.twig') }}
{% endblock %}
